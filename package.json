{
	"name": "cypress-challenge",
	"version": "1.0.0",
	"description": "Challenge de Cypress Parallelization con Cucumber y TypeScript.",
	"scripts": {
		"start": "react-scripts start",
		"test": "cypress run",
		"test:parallel": "cypress run --parallel --record --key 47298b7b-69d6-42f5-a8da-9b9ed90784e4",
		"cypress:open": "cypress open"
	},
	"dependencies": {
		"@badeball/cypress-cucumber-preprocessor": "^22.0.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "6.14.2",
		"react-scripts": "^5.0.1",
		"ts-node": "^10.9.2"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.26.0",
		"@cypress/browserify-preprocessor": "^3.0.0",
		"@cypress/webpack-preprocessor": "^5.0.0",
		"@types/cucumber": "^7.0.2",
		"@types/node": "^22.15.3",
		"cypress": "^14.0.0",
		"cypress-mochawesome-reporter": "^3.8.2",
		"multiple-cucumber-html-reporter": "^3.9.2",
		"typescript": "^5.1.6",
		"esbuild": "^0.25.3",
		"@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
		"@badeball/cypress-cucumber-preprocessor": "^15.0.0"
	},
	"cypress-cucumber-preprocessor": {
		"nonGlobalStepDefinitions": false,
		"stepDefinitions": "cypress/integration/step_definitions/**",
		"failFast": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}